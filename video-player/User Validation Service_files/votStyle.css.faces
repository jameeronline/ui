    font-weight: bold;
    font-size:14px;
    display: flex;
    align-items: center;
}
.align-center{
	display: flex;
    align-items: center;
    justify-content: center;	
}
.slide-title{
	font-size:22px;
}
.green-box{
    display: flex;
    justify-content: center;
    align-items: center;
	background-color: #00ab67 !important;
    color: #ffffff;
    font-size: 22px !important;
    text-align: center;
    font-weight: bold;
    min-height: 50px !important;
}

.left-box{
margin-right: 15px;	
margin-left: 15px;
}
.green-box img{
	margin-bottom:10px;
}
