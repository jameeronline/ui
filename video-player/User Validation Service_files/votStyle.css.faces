input[type=submit]:disabled, input[type=button]:disabled, button:disabled {
    color: #999 !important;
    border: 1px #bbb solid !important;
    background: #e8e8e8 !important;
    border-radius: 0 !important;
    vertical-align: middle;
}
@media screen and (max-width: 540px) {

input[type=checkbox], input[type=radio] {
    width: 50px;
    height: 50px;
}
input[type=submit]:disabled, input[type=button]:disabled, button:disabled {
    width: 100% !important;
}
.green-color, .rtl .green-color, .green-color:link, .green-color:active, .green-color:visited, .green-color:hover, input[type='submit'], input[type='button'], .rtl input[type='submit'], .rtl input[type='button'] {
    width: 100% !important;
}

    .btn-lg, .rtl .btn-lg,.btn-lg:link,.btn-lg:active,.btn-lg:visited,.btn-lg:hover {
    width: 100% !important;
    margin-bottom:15px !important;
}
#dialogbox {
    display: none;
    position: fixed;
    background: #fff;
    border-radius: 7px;
    width: 100%;
    z-index: 1032;
}
}
.videoBox{
    display: flex;
    justify-content: center;
    align-items: center;
}

.close{
	opacity: 1 !important;
}
.close-link:lang(en){
	display: block;
    width: 50px;
    height: 50px;
    position: absolute;
    top: 0px;
    right: 0px;
}
.close-link:lang(ar){
	display: block;
    width: 50px;
    height: 50px;
    position: absolute;
    top: 0px;
    left: 0px;
}
.close:before{
    color: #00ab67;
    content: '\e808';
    font-size: 22px;
    font-family: "fontello";
    font-style: normal;
    font-weight: normal;
    speak: none;
    display: inline-block;
    text-decoration: inherit;
    margin-right: .5em;
    text-align: center;
    font-variant: normal;
    text-transform: none;
    line-height: 1.5em;
    margin-left: -1.3em;
    -webkit-font-smoothing: antialiased;	
}
ul.bullet{
	padding-left: 0px;
    padding-right: 0px;
    margin-right: 30px;
    margin-left: 30px;  
}
ul.bullet li{
	list-style-image: url(); 
	list-style:none;
}
ul.bullet:lang(en) li:before{
    margin-right: .5em;
    margin-left: -1.3em;
}
.rtl ul.bullet:lang(ar) li:before{
    margin-left: .5em;
    margin-right: -1.3em;
}
.rtl ul.bullet li{
    content: "";
    background-image: url(/wps/PA_UValidationService/resources/images/left-side.png);
    background-size: 3%;
    background-repeat: no-repeat;
    background-position: right 2px;
    padding-right: 44px;
    line-height: 30px;
}
ul.bullet li{
    content: "";
    background-image: url(/wps/PA_UValidationService/resources/images/right-side.png);
    background-size: 3%;
    background-repeat: no-repeat;
    background-position: left 2px;
    padding-left: 44px;
    line-height: 30px;
}
